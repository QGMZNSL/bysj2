package bysj.zxpc.bean;

public class CourseBean {
	int courseId;
	String courseName;
	int CourseTime;
	int CourseGra;
	String CourseTea;
	String CourseBz;
	public CourseBean() {
		super();
		// TODO Auto-generated constructor stub
	}
	public CourseBean(int courseId, String courseName, int courseTime, int courseGra, String courseTea,
			String courseBz) {
		super();
		this.courseId = courseId;
		this.courseName = courseName;
		CourseTime = courseTime;
		CourseGra = courseGra;
		CourseTea = courseTea;
		CourseBz = courseBz;
	}
	public int getCourseId() {
		return courseId;
	}
	public void setCourseId(int courseId) {
		this.courseId = courseId;
	}
	public String getCourseName() {
		return courseName;
	}
	public void setCourseName(String courseName) {
		this.courseName = courseName;
	}
	public int getCourseTime() {
		return CourseTime;
	}
	public void setCourseTime(int courseTime) {
		CourseTime = courseTime;
	}
	public int getCourseGra() {
		return CourseGra;
	}
	public void setCourseGra(int courseGra) {
		CourseGra = courseGra;
	}
	public String getCourseTea() {
		return CourseTea;
	}
	public void setCourseTea(String courseTea) {
		CourseTea = courseTea;
	}
	public String getCourseBz() {
		return CourseBz;
	}
	public void setCourseBz(String courseBz) {
		CourseBz = courseBz;
	}
	

}
